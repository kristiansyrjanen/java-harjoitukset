import java.text.DecimalFormat;
import java.util.Scanner;

public class Palkka {

	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		DecimalFormat desimaalit = new DecimalFormat("0.00");
		
		int palkka, ika;
		double veroprosentti, verotettu, vakuutus, elakemaksu, lopullinen;
		
		System.out.print("Anna palkka: ");
		palkka = input.nextInt();
		System.out.print("Anna veroprosentti: ");
		veroprosentti = input.nextDouble();
		System.out.print("Anna ikä: ");
		ika = input.nextInt();
		
		vakuutus = palkka * 0.0115;
		verotettu = (double) palkka * veroprosentti / 100.0;
		
		if (ika >= 53) {
			elakemaksu = palkka * 0.0705;
			lopullinen = palkka - verotettu - vakuutus - elakemaksu;
			System.out.println("Bruttopalkka " + palkka);
			System.out.println("Veron osuus " + desimaalit.format(verotettu));
			System.out.println("Työeläkevakuutusmaksun osuus  " + desimaalit.format(elakemaksu));
			System.out.println("Työttömyysvakuutuksen osuus " + desimaalit.format(vakuutus));
			System.out.println("Käteen jää " + desimaalit.format(lopullinen));
		} else {
			elakemaksu = palkka * 0.0555;
			lopullinen = palkka - verotettu - vakuutus - elakemaksu;
			System.out.println("Bruttopalkka " + palkka);
			System.out.println("Veron osuus " + desimaalit.format(verotettu));
			System.out.println("Työeläkevakuutusmaksun osuus  " + desimaalit.format(elakemaksu));
			System.out.println("Työttömyysvakuutuksen osuus " + desimaalit.format(vakuutus));
			System.out.println("Käteen jää " + desimaalit.format(lopullinen));
		}
			
		
	}

}
