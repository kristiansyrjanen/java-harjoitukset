import java.text.DecimalFormat;
import java.util.Scanner;

public class Ylevero {

	public static void main(String[] args) {
		/*Anna vuositulosi ja ikäsi: 9000 21 
		Ylevero on 0,00 */
		
		Scanner input = new Scanner(System.in);
		DecimalFormat desimaalit = new DecimalFormat("0.00");
		
		int ika;
		double vuositulot, ylevero;
		
		
		System.out.print("Anna vuositulosi ja ikäsi: ");
		vuositulot = input.nextDouble();
		ika = input.nextInt();
		ylevero = vuositulot * 0.68/100;
		
		if (ylevero<70){
            ylevero = 0;
        }
        if (ika <18){
            ylevero = 0;
        }
        
        if (ylevero >140){
            ylevero = 140;
        }
        
        System.out.println("Ylevero on "+desimaalit.format(ylevero));
		
	}

}
